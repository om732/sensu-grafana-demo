{
  "checks": {
    "check_java_heapmemory_pcnt": {
      "command": "jolokia-heapmemory-check.rb -u :::java.jolokia_url::: -w :::java.heapmemory_warn::: -c :::java.heapmemory_crit:::",
      "subscribers": [
        "java"
      ],
      "handlers": [
        "mailer-crit",
        "mailer-warn"
      ],
      "interval": 60,
      "occurrences": 6,
      "refresh": 86400,
      "custom_summary": "HeapMemory使用率の監視"
    }
  }
}
